{
  "manifest_version": 2,
  "name": "Savory",
  "version": "0.1.0",
  "description": "Bookmark Manager Extension for Chrome",
  "permissions": [
    "storage",
    "bookmarks"
  ],
  "chrome_url_overrides": {
    "bookmarks": "src/bookmarks.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
